;; Testing "split" function

;; split for string
(split "abc-abc" "-")
;=>("abc" "abc")
(split "abc-abc" " ")
;=>("abc-abc")
(split "abc-abc" "")
;=>("a" "b" "c" "-" "a" "b" "c")
(split "" "-")
;=>("")
(split "abc abc-abc" "-")
;=>("abc abc" "abc")
(split "abc abc-abc" " ")
;=>("abc" "abc-abc")
;; compatible with first
(first (split "abc-abc" "-"))
;=>"abc"