(/ 0 0)
;=>nil
;/^.*integer divide by zero$

(/ 1 0)
;=>nil
;/^.*integer divide by zero$

(+ 1 :hello)
;=>nil
;/^.*using string as type int$

(+ 1 "hello")
;=>nil
;/^.*using string as type int$

(try (/ 1 0))
;=>nil
;/^.*integer divide by zero$

(try (/ 1 0) (catch e (get e :err)))
;=>"github.com/jig/lisp/lib/core[/]: runtime error: integer divide by zero"
;/^$

(context (/ 1 0))
;=>nil
;/^.*runtime error: integer divide by zero.*$

